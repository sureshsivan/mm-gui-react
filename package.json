{
  "name": "mm-gui-react",
  "version": "1.0.0",
  "description": "react based GUI for mm app",
  "main": "index.js",
  "author": "suresh.sivanantham@gmail.com",
  "license": "MIT",
  "scripts": {
    "_checkdeployenv": "node techops/scripts/nodejs/validate-deploy-env.js",
    "_applyperm": "chmod a+x -R ./techops/scripts/awscli/*.sh",
    "_prepcfstack": "node techops/scripts/nodejs/yaml-merger-cf-template",
    "_uploadartifacts": "./techops/scripts/awscli/upload_assets_to_s3.sh",
    "_createorupdatestack": "./techops/scripts/awscli/create_or_update_stack.sh",
    "_webpack": "webpack --config webpack.config.js --progress --profile --bail",
    "start": "webpack-dev-server --port 8888",
    "build": "rimraf dist && npm run _webpack",
    "prepdeploy": "npm run _checkdeployenv && npm run _applyperm && rimraf .build && npm run _prepcfstack",
    "deploy": "npm run _createorupdatestack && npm run _uploadartifacts",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v8-suresh/mm-gui-react.git"
  },
  "keywords": [
    "react",
    "redux",
    "serverless",
    "auth0",
    "learn",
    "bootstrap",
    "webpack",
    "aws",
    "cloudformation",
    "moneymanager"
  ],
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "2.5.4",
    "sass-loader": "^5.0.1",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.64.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "1.16.3",
    "webpack-merge": "2.6.1"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "json-refs": "2.1.6",
    "yaml-js": "0.1.4"
  }
}
